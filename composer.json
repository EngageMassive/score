{
  "name": "takt/score",
  "type": "package",
  "version": "1.0.0",
  "description": "Score framework for Prelude and Acorn-based WordPress themes.",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Lee",
      "email": "robert@takt.com",
      "homepage": "https://github.com/ssang"
    }
  ],
  "autoload": {
    "psr-4": {
